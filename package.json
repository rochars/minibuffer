{
  "name": "minibuffer",
  "version": "0.2.0",
  "description": "A minimalist buffer reader and writer.",
  "homepage": "https://github.com/rochars/minibuffer",
  "author": "Rafael da Silva Rocha <rocha.rafaelsilva@gmail.com>",
  "license": "MIT",
  "main": "./dist/minibuffer.cjs.js",
  "module": "./index.js",
  "es2015": "./dist/minibuffer.js",
  "jsdelivr": "./dist/minibuffer.min.js",
  "unpkg": "./dist/minibuffer.min.js",
  "types": "./index.d.ts",
  "engines": {
    "node": ">=8"
  },
  "keywords": [
    "buffer",
    "io",
    "read",
    "write",
    "8-bit",
    "16-bit",
    "24-bit",
    "32-bit",
    "40-bit",
    "48-bit",
    "64-bit",
    "integer",
    "floating-point",
    "ASCII"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/rochars/minibuffer.git"
  },
  "bugs": {
    "url": "https://github.com/rochars/minibuffer/issues"
  },
  "directories": {
    "dist": "dist",
    "externs": "externs"
  },
  "files": [
    "dist",
    "externs",
    "index.js",
    "index.d.ts",
    "LICENSE",
    "README.md",
    "AUTHORS.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "lint": "jshint index.js externs",
    "test": "nyc ./node_modules/mocha/bin/_mocha test/api --recursive -R dot",
    "test-esm": "node ./node_modules/mocha/bin/_mocha test/api --esm --recursive -R dot",
    "test-min": "node ./node_modules/mocha/bin/_mocha test/api --min --recursive -R dot",
    "test-cjs": "node ./node_modules/mocha/bin/_mocha test/api --cjs --recursive -R dot",
    "test-tsc": "tsc ./test/TypeScript/index.ts",
    "test-dist": "npm run test-min && npm run test-cjs && npm run test-esm",
    "rollup-bundle": "rollup -c && npm run test-dist && npm run test-tsc",
    "doc": "./node_modules/.bin/jsdoc index.js -d docs/api -t node_modules/docdash",
    "build": "npm run lint && npm test && npm run rollup-bundle && npm run doc",
    "coverage": "nyc report --reporter=lcov > coverage.lcov && codecov"
  },
  "devDependencies": {
    "binary-data-types": "^0.1.0",
    "codecov": "^3.0.2",
    "docdash": "^0.4.0",
    "esm": "^3.0.51",
    "google-closure-compiler-js": "^20180610.0.0",
    "jsdoc": "^3.5.5",
    "jshint": "^2.9.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^12.0.2",
    "rollup": "^0.61.2",
    "rollup-plugin-closure-compiler-js": "^1.0.6",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "byte-data": "^14.0.1"
  }
}
